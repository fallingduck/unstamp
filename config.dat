# Enable or disable logging of server activity
LOGGING     true


# The name by which the server will identify itself to other servers
HOSTNAME    "example.com"


# MAIL TRANSFER AGENT
# This is the information for the MTA, which receives incoming mail from the
# outside.
MTA_BIND        [ ["::1", 25], ["127.0.0.1", 25] ]
MTA_MAX_SIZE    10485760
